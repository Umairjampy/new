    "# Calculator Task\n",
    "\n",
    "a=input(\"Please enter first number \")\n",
    "b=input(\"Please enter second number \")\n",
    "opr=input(\"enter operator\")\n",
    "a=int(a)\n",
    "b=int(b)\n",
    "if(opr=='+'):\n",
    "    print(a+b)\n",
    "elif(opr=='-'):\n",
    "    print(a-b)\n",
    "elif(opr=='*'):\n",
    "    print(a*b)\n",
    "elif(opr=='/'):\n",
    "    print(a/b)\n",
    "elif(opr=='%'):\n",
    "    print(a%b)\n",
    "else:\n",
    "    print('invaid')"
   ]
